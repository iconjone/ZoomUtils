{
	"manifest_version": 2,
	"name": "Zoom Utils",
	"description": "Zoom Utils for Zoom University",
	"version": "1.3",
	"permissions": [
		"storage"
	],
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "img/icon48.png",
			"38": "img/icon48.png"
		},
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"img/icon128.png"
	],
	"web_accessible_resources": [
  "data.json"
],
"background": {
	"scripts": ["js/background.js"],
	"persistent": false
}
}
